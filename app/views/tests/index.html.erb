<h2>All tests</h2>

<%= link_to 'Create new test', new_test_path %>

<table class="table table-striped">
  <tr>
    <th>Name</th>
    <th>Link</th>
    <th>Questions</th>
    <th>Results</th>
    <th>Actions</th>
  </tr>

  <% @tests.each do |test| %>
    <tr>
      <td><%= link_to test.name, '#' %></td>
      <td><%= link_to 'Copy link', '#' %></td>
      <td></td>
      <td></td>
      <td></td>
    </tr>
  <% end %>
</table>
